{
    "JSON-zain.json.autorefresh": true,
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "recentlyUsed",
    "files.autoGuessEncoding": true,
    "files.autoSave": "onWindowChange",
    "files.trimTrailingWhitespace": true,
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.list.smoothScrolling": true,
    "workbench.quickOpen.preserveInput": true,
    "workbench.startupEditor": "none",
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.editor.autoLockGroups": {
        "default": true,
        "jupyter-notebook": true,
        "vscode.audioPreview": true,
        "vscode.videoPreview": true,
        "jsProfileVisualizer.cpuprofile.table": true,
        "gc-excelviewer-csv-editor": true,
        "gc-excelviewer-excel-editor": true,
        "gitlens.rebase": true,
        "workbench.input.interactive": true,
        "mainThreadWebview-markdown.preview": true
    },
    "editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToImplementation",
    "editor.gotoLocation.alternativeDeclarationCommand": "editor.action.peekDeclaration",
    "editor.gotoLocation.alternativeDefinitionCommand": "editor.action.peekDefinition",
    "editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToReferences",
    "editor.gotoLocation.alternativeTypeDefinitionCommand": "editor.action.peekTypeDefinition",
    "editor.guides.bracketPairs": true,
    "files.insertFinalNewline": true,
    "workbench.editor.wrapTabs": true,
    "workbench.settings.editor": "json",
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "workbench.settings.useSplitJSON": true,
    "window.autoDetectColorScheme": true,
    "window.commandCenter": true,
    "explorer.fileNesting.enabled": true,
    "search.experimental.notebookSearch": true,
    "search.quickOpen.includeSymbols": true,
    "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
    "search.searchEditor.reusePriorSearchConfiguration": true,
    "search.seedOnFocus": true,
    "search.seedWithNearestWord": true,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "debug.allowBreakpointsEverywhere": true,
    "debug.autoExpandLazyVariables": true,
    "debug.console.acceptSuggestionOnEnter": "on",
    "debug.console.closeOnEnd": true,
    "debug.openExplorerOnEnd": false,
    "debug.showBreakpointsInOverviewRuler": true,
    "debug.terminal.clearBeforeReusing": true,
    "scm.alwaysShowActions": true,
    "scm.alwaysShowRepositories": true,
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "extensions.closeExtensionDetailsOnViewChange": false,
    "extensions.experimental.deferredStartupFinishedActivation": true,
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "terminal.integrated.shellIntegration.suggestEnabled": true,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.tabs.defaultColor": "terminal.ansiBlue",
    "terminal.integrated.tabs.defaultIcon": "terminal-powershell",
    "terminal.integrated.tabs.title": "${workspaceFolder}",
    "task.quickOpen.skip": true,
    "problems.showCurrentInStatus": true,
    "notebook.lineNumbers": "on",
    "notebook.output.scrolling": true,
    "notebook.output.wordWrap": true,
    "interactiveWindow.restore": true,
    "mergeEditor.diffAlgorithm": "smart",
    "interactiveSession.editor.wordWrap": "on",
    "application.experimental.rendererProfiling": true,
    "cSpell.caseSensitive": true,
    "cSpell.autoFormatConfigFile": true,
    "cSpell.showAutocompleteSuggestions": true,
    "cSpell.noConfigSearch": true,
    "css.format.spaceAroundSelectorSeparator": true,
    "less.format.spaceAroundSelectorSeparator": true,
    "scss.format.spaceAroundSelectorSeparator": true,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.useInlineCompletions": true,
    "emmet.triggerExpansionOnTab": true,
    "eslint.experimental.useFlatConfig": true,
    "eslint.format.enable": true,
    "eslint.ignoreUntitled": true,
    "eslint.lintTask.enable": true,
    "csv-preview.lineNumbers": "source",
    "csv-preview.resizeColumns": "first",
    "csv-preview.openStdin": true,
    "csv-preview.skipComments": true,
    "git.closeDiffOnOperation": true,
    "git.enableSmartCommit": true,
    "git.ignoreLegacyWarning": true,
    "git.ignoreMissingGitWarning": true,
    "git.ignoreLimitWarning": true,
    "gitlens.views.repositories.includeWorkingTree": true,
    "gitlens.views.repositories.compact": true,
    "html.format.indentHandlebars": true,
    "html.format.templating": true,
    "html.format.indentInnerHtml": true,
    "vsintellicode.features.python.deepLearning": "enabled",
    "json.format.keepLines": true,
    "npm.enableRunFromFolder": true,
    "npm.runSilent": true,
    "npm.packageManager": "npm",
    "npm-intellisense.packageSubfoldersIntellisense": true,
    "npm-intellisense.scanDevDependencies": true,
    "npm-intellisense.showBuildInLibs": true,
    "powershell.buttons.showPanelMovementButtons": true,
    "powershell.codeFormatting.avoidSemicolonsAsLineTerminators": true,
    "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
    "powershell.codeFormatting.useConstantStrings": true,
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "powershell.debugging.createTemporaryIntegratedConsole": true,
    "powershell.developer.editorServicesWaitForDebugger": true,
    "powershell.integratedConsole.forceClearScrollbackBuffer": true,
    "prettier.bracketSameLine": true,
    "prettier.enableDebugLogs": true,
    "prettier.insertPragma": true
}
